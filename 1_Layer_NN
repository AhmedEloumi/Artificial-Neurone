      "cell_type": "code",
      "source": [
        " new_plant=np.array([3,3])\n",
        " x1=np.linspace(-1,4,100)\n",
        " x2=(-W[0]*x1-b)/W[1]\n",
        " plt.scatter(X[:,0], X[:, 1], c=y, cmap='summer')\n",
        " plt.scatter(new_plant[0], new_plant[1], c='b')\n",
        " plt.plot(x1,x2,c='orange',lw=3)\n",
        " plt.show()\n",
        " predict(new_plant, W, b)"
