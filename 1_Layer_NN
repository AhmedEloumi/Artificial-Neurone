        "X, y = make_blobs(n_samples=100, n_features=2, centers=2, random_state=0)\n",
        "y = y.reshape((y.shape[0], 1))\n",
        "\n",
        "print('dim de X:', X.shape)\n",
        "print('dim de y:', y.shape)\n",
        "\n",
        "plt.scatter(X[:,0], X[:, 1], c=y, cmap='summer')\n",
        "plt.show()"
