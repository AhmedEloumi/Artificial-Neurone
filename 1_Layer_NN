      "cell_type": "markdown",
      "source": [
        "#**model functions**"
      ],
      "metadata": {
        "id": "oavoElXYOVRw"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def initialisation(X):\n",
        "  W=np.random.randn(X.shape[1],1)\n",
        "  b=np.random.randn(1)\n",
        "  return(W,b)"
      ],
      "metadata": {
        "id": "xju5G_UX68cY"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def model(X, W, b):\n",
        "    Z = X.dot(W) + b\n",
        "    A = 1 / (1 + np.exp(-Z))\n",
        "    return A\n"
      ],
      "metadata": {
        "id": "ts1VpEam7RxP"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def log_loss(A, y):\n",
        "  return  1 / len(y) * np.sum(-y * np.log(A) - (1 - y) * np.log(1 - A))"
      ],
      "metadata": {
        "id": "l3TDsdyp8Ok6"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "def gradients(A, X, y):\n",
        "    dW = 1/len(y) * np.dot(X.T, A - y)\n",
        "    db = 1 / len(y) * np.sum(A - y)\n",
        "    return (dW, db)"
      ],
      "metadata": {
        "id": "C6WXX5G59yGa"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def update(W,b,learning_rate,dW,db):\n",
        "  W=W-learning_rate*dW\n",
        "  b=b-learning_rate*db\n",
        "  return(W,b)\n"
      ],
      "metadata": {
        "id": "f0ennfoPDPdA"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def predict(X, W, b):\n",
        "    A = model(X, W, b)\n",
        "    #print(A)\n",
        "    return A >= 0.5"
      ],
      "metadata": {
        "id": "CQDkhV8OPV3h"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import accuracy_score"
      ],
      "metadata": {
        "id": "kgObUentPkIG"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def artificial_neuron(X, y, learning_rate = 0.1, n_iter = 100):\n",
        "    # initialisation W, b\n",
        "    W, b = initialisation(X)\n",
        "\n",
        "    Loss = []\n",
        "\n",
        "    for i in range(n_iter):\n",
        "        A = model(X, W, b)\n",
        "        Loss.append(log_loss(A, y))\n",
        "        dW, db = gradients(A, X, y)\n",
        "        W, b =update(W,b,learning_rate,dW,db)\n",
        "        \n",
        "    y_pred = predict(X, W, b)\n",
        "    print(accuracy_score(y, y_pred))\n",
        "\n",
        "    plt.plot(Loss)\n",
        "    plt.show()\n",
        "    return(W,b) "
      ],
      "metadata": {
        "id": "a0dDrY9dDuun"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "W,b=artificial_neuron(X,y)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
